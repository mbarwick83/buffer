{
    "name": "mbarwick83/buffer",
    "description": "Simple Buffer API package for Laravel 5.*.",
    "keywords": [
        "buffer",
        "api",
        "laravel"
    ],
    "homepage": "https://github.com/mbarwick83/buffer",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barwick",
            "email": "mike@barwickandco.com"
        }
    ],
    "require": {
        "php" : ">=5.5.0",        
        "symfony/psr-http-message-bridge": "~1.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Mbarwick83\\Buffer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mbarwick83\\Buffer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
